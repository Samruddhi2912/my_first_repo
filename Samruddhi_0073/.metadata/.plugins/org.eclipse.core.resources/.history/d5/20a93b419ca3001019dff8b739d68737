
public class FuncOverLoad {
  
	public static int add(int x, int y) {
		return (x+y);
	}
	public int max(int x, int y, int z) {
		if(x>y && x>z)
		return x; 
		else if(y>x && y>z) {
			return y;
		}
		else {
			return z;
		}
	}
  
    public float max(float x, float y ) {
    	return(Math.max(x, y));
    }
}